import java.awt.Graphics;

import javax.swing.JPanel;

public class Main extends JPanel {

	private static final long serialVersionUID = 1L;
	
	@Override
	protected void paintComponent(Graphics g) {
		super.paintComponent(g);
		int min_rx = 10;
		int max_rx = 50;
		int min_ox = 10;
		int max_ox = 690;
		int dx = 20;
		double vysledekx = (double)(dx - min_rx) / (max_rx - min_rx) * (max_ox - min_ox) + min_ox;

		int min_ry = 20;
		int max_ry = 60;
		int min_oy = 10;
		int max_oy = 490;
		int dy = 30;
		double vysledeky = (double)(dy - min_ry) / (max_ry - min_ry) * (max_oy - min_oy) + min_oy;
		
		System.out.println(vysledekx + ", " + vysledeky);
	}
	
}
